<div class="container">
    <h3 class="text-center mt-2"><b>DANH SÁCH Y TÁ</b></h3>
    <table id="example" class="table table-striped" style="width:100%;background-color: rgb(204, 248, 204);">
        <thead>
            <tr>
                <th>Tên y tá</th>
                <th>Giới tính</th>
                <th>Chuyên khoa</th>
                <th>Điện thoại</th>
                <th>Chức vụ</th>
            </tr>
        </thead>
        <tbody>
            {{#each doctors}}
            <tr>
                <td><a href="/y-ta/{{ID}}-{{Name}}">{{Name}}</a></td>
                <td>{{Gender}}</td>
                <td>{{Specialist}}</td>
                <td>{{Phone}}</td>
                <td>{{Title}}</td>
            </tr>
            {{/each}}
        <tfoot>
            <tr>

            </tr>
        </tfoot>
    </table>
</div>